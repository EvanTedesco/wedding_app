<div class="container">
  <div class="row">
    <div class='list-group gallery'>
      <% @photos.each do |photo| %>
        <div class='col-sm-4 col-xs-6 col-md-3 col-lg-3'>
          <a class="thumbnail fancybox" rel="ligthbox" href="<%= photo.url %>">
            <%= image_tag photo.url, class: "img-responsive",  height: '193', width: '132' %>
          </a>
        </div>
      <% end %>
    </div>
  </div>
</div>