<header>
  <%= link_to 'Home', root_path %>
  <%= link_to 'Our Story', stories_path %>
  <%= link_to 'RSVP', new_rsvp_path %>
  <%= link_to 'Photos', photos_path %>
  <%= link_to 'Wedding Party', wedding_parties_path %>
  <%= link_to 'The Deets', deets_path %>
  <% if session[:user_id] == nil %>
    <%= link_to 'Login', new_session_path %>
  <% else %>
    <%= link_to 'Logout', "/sessions/#{current_user.id}", method: :delete %>
  <% end %>
</header>
<% if current_user && current_user.admin? %>
  <%= render 'shared/admin_nav' %>
<% end %>